{
    "manifest_version": 3,
    
    "name": "YouTube Anti Anti-Adblock",
    "version": "0.9.1",
    "description": "Removes the anti adblock on YouTube",
    "author": "Wojciech Warpechowski",
    "homepage_url": "https://github.com/wojciesh/YouTube-Anti-Anti-Adblock",
    
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"], 
        "js": ["script.js"],
        "run_at": "document_start"
      }
    ],
  
    "permissions": [
      "activeTab"
    ]
  }
  